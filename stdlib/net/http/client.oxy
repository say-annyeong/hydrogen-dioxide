// Basic HTTP client functionality

// Simple struct to hold basic HTTP response info
struct HttpResponse {
    status_code: int,
    // headers: Dict, // TODO: Add Dict type later
    body: string,
}

// Performs a simple HTTP GET request.
// Returns an HttpResponse struct containing status_code and body.
// Error handling and response parsing are minimal.
// Uses the underlying Rust built-in function __http_get.
fn get(url: string) -> HttpResponse {
    let response_instance = __http_get(url); // Call the Rust built-in
    // TODO: Improve error handling if __http_get returns non-struct
    return response_instance;
}

