// stdlib/cmp/ord.oxy

// Comparison and ordering functions

// Returns the maximum of two numbers
// Supports int and float comparison
fn max(a, b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

// Returns the minimum of two numbers
// Supports int and float comparison
fn min(a, b) {
    if (a < b) {
        return a;
    } else {
        return b;
    }
} 